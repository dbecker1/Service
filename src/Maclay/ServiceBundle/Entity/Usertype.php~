<?php

namespace Maclay\ServiceBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Usertype
 */
class Usertype
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $description;

    /**
     * @var boolean
     */
    private $canviewall;

    /**
     * @var boolean
     */
    private $canenterrecord;

    /**
     * @var boolean
     */
    private $canapprove;

    /**
     * @var boolean
     */
    private $hasadmin;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set description
     *
     * @param string $description
     * @return Usertype
     */
    public function setDescription($description)
    {
        $this->description = $description;

        return $this;
    }

    /**
     * Get description
     *
     * @return string 
     */
    public function getDescription()
    {
        return $this->description;
    }

    /**
     * Set canviewall
     *
     * @param boolean $canviewall
     * @return Usertype
     */
    public function setCanviewall($canviewall)
    {
        $this->canviewall = $canviewall;

        return $this;
    }

    /**
     * Get canviewall
     *
     * @return boolean 
     */
    public function getCanviewall()
    {
        return $this->canviewall;
    }

    /**
     * Set canenterrecord
     *
     * @param boolean $canenterrecord
     * @return Usertype
     */
    public function setCanenterrecord($canenterrecord)
    {
        $this->canenterrecord = $canenterrecord;

        return $this;
    }

    /**
     * Get canenterrecord
     *
     * @return boolean 
     */
    public function getCanenterrecord()
    {
        return $this->canenterrecord;
    }

    /**
     * Set canapprove
     *
     * @param boolean $canapprove
     * @return Usertype
     */
    public function setCanapprove($canapprove)
    {
        $this->canapprove = $canapprove;

        return $this;
    }

    /**
     * Get canapprove
     *
     * @return boolean 
     */
    public function getCanapprove()
    {
        return $this->canapprove;
    }

    /**
     * Set hasadmin
     *
     * @param boolean $hasadmin
     * @return Usertype
     */
    public function setHasadmin($hasadmin)
    {
        $this->hasadmin = $hasadmin;

        return $this;
    }

    /**
     * Get hasadmin
     *
     * @return boolean 
     */
    public function getHasadmin()
    {
        return $this->hasadmin;
    }
}
