<?php

namespace Maclay\ServiceBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Clubinfo
 */
class Clubinfo
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $clubname;

    /**
     * @var integer
     */
    private $clubsponsoruserid;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set clubname
     *
     * @param string $clubname
     * @return Clubinfo
     */
    public function setClubname($clubname)
    {
        $this->clubname = $clubname;

        return $this;
    }

    /**
     * Get clubname
     *
     * @return string 
     */
    public function getClubname()
    {
        return $this->clubname;
    }

    /**
     * Set clubsponsoruserid
     *
     * @param integer $clubsponsoruserid
     * @return Clubinfo
     */
    public function setClubsponsoruserid($clubsponsoruserid)
    {
        $this->clubsponsoruserid = $clubsponsoruserid;

        return $this;
    }

    /**
     * Get clubsponsoruserid
     *
     * @return integer 
     */
    public function getClubsponsoruserid()
    {
        return $this->clubsponsoruserid;
    }
}
