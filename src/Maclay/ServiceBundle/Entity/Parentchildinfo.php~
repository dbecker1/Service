<?php

namespace Maclay\ServiceBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Parentchildinfo
 */
class Parentchildinfo
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $parentuserid;

    /**
     * @var integer
     */
    private $childuserid;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set parentuserid
     *
     * @param integer $parentuserid
     * @return Parentchildinfo
     */
    public function setParentuserid($parentuserid)
    {
        $this->parentuserid = $parentuserid;

        return $this;
    }

    /**
     * Get parentuserid
     *
     * @return integer 
     */
    public function getParentuserid()
    {
        return $this->parentuserid;
    }

    /**
     * Set childuserid
     *
     * @param integer $childuserid
     * @return Parentchildinfo
     */
    public function setChilduserid($childuserid)
    {
        $this->childuserid = $childuserid;

        return $this;
    }

    /**
     * Get childuserid
     *
     * @return integer 
     */
    public function getChilduserid()
    {
        return $this->childuserid;
    }
}
